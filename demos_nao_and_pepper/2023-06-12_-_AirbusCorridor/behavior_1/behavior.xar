def generateListSentencesASR():
    """
    generate all sentences for speech recognition, return the list and the id associated
    """

    listQuestion = (
        ["I have an appointment with", "I would like to see", "I'm looking for", "I'm looking for the office of"],
        ["J'ai rendez vous avec", "J'aimerais voir", "Je cherche le bureau de", "Je cherche"],
        )

    # a list of programming keyword/action name => translation.
    # don't change keyword or you'll need to change in the code
    listExtra = {
                "check": ("Let's do a check","On se fait un check"),
                "check": ("let's check","fais moi un check"),
                "check": ("a check","un check"),
                "repeat": ("repeat","répête"),
                "repeat": ("repeat please","répête s'il te plait"),
                "repeat": ("could you repeat","pourrais tu répêter"),
                "repeat": ("could you repeat please","pourrais tu répêter s'il te plait"),
                "switch_to_en": ("please speak english","s'il te plait parle en anglais"),
                "switch_to_en": ("speak english","parle en anglais"),
                "switch_to_fr": ("please speak french","s'il te plait parle en francais"),
                "switch_to_fr": ("speak french","parle en francais"),
    }

    idxLang = getLangIndex()


    o = []
    names = []
    for q in listQuestion[idxLang]:
        for e in listEmployee:
            f,l = e.split(" ")
            o.append( q + " " + e )
            o.append( q + " " + f )
            o.append( q + " Monsieur " + l )
            o.append( q + " Madame " + l )
            for i in range(4):
                names.append(e)
    if 1:
        for k,v in listExtra.items():
            o.append(v[idxLang])
            names.append(k)
    return o,names

def getInstructionSentence(strPersonName):
    idxLang = getLangIndex()
    if idxLang == 1:
        strWord = pickWord(["facile","pas loin","ici", "par la", ""])
        return "le bureau de %s ? c'est %s: " % (strPersonName, strWord)
    strWord = pickWord(["easy","near","simple", "there"])
    return "the %s's office ? it's %s: " % (strPersonName, strWord)

def findID(s):
    print("INF: findID '%s'" % s)
    sentences,names = generateListSentencesASR()
    print("INF: findID: sentences: %s" % str(sentences))
    try:
        idx = sentences.index(s)
    except ValueError as err:
        return "inconnu",-1
    return names[idx]

def nameToOffice(name):
    idx = listEmployee.index(name)
    office = listOffice[idx]
    return office

def nameToPlan(name):
    office = nameToOffice(name)
    try:
        plan = list(listPlan[office][:])
        print("INF: nameToPlan: plan: %s" % str(plan))
        # transform text to the right lang
        plan[0] = plan[0][getLangIndex()]
        return plan
    except KeyError as err:
        print("WRN: nameToPlan: err: " + str(err) )
    return "lieu inconnu", -1, -1

def pickWord(words):
    idx = random.randint(0,len(words)-1)
    return words[idx]



tts = naoqi.ALProxy("ALTextToSpeech")

def getLangIndex():
    strCurrentLang = tts.getLanguage()
    idxLang = 0
    if strCurrentLang == "French":
        idxLang = 1
    return idxLang

def getTextMultiLang(strTextID):
    idxLang = getLangIndex()
    return listTextMulti[strTextID][idxLang]

